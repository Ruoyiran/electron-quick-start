{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron with ReactJS application",
  "main": "src/electron/main.js",
  "scripts": {
    "start:app": "zx ./scripts/app.mjs",
    "start:electron": "unset NODE_OPTIONS && electron ./src/electron/main.js",
    "start:server": "live-server ./dist --cors --port=9221 --no-browser --watch=index.html,dist",
    "start:dev": "cross-env NODE_ENV=development webpack --config ./scripts/webpack.config.js --watch --progress",
    "eslint": "eslint -c .eslintrc.js"
  },
  "repository": "https://github.com/ruoyiran/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "ruoyiran",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.2.1",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unicorn": "^54.0.0",
    "html-webpack-plugin": "^5.6.0",
    "live-server": "^1.2.0",
    "prettier": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.3",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "zx": "^8.1.4"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
