  "compilerOptions": {
    "target": "ES2018",
    "sourceMap": true,
    "module": "commonjs",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "declaration": false,
    "resolveJsonModule": true
